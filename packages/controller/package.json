{
  "name": "@cloudydaiyz/furu-controller",
  "description": "Playwright controller for the furu project",
  "version": "1.0.0",
  "author": "Kylan Duncan",
  "keywords": [
    "furu",
    "playwright"
  ],
  "license": "ISC",
  "scripts": {
    "build": "tsc && pnpm exec playwright install && node scripts/generate-injected.mjs",
    "inspector": "npm run build && node dist/sandbox/inspector.js",
    "executor": "npm run build && node --experimental-repl-await --experimental-vm-modules dist/sandbox/executor.js",
    "start": "npm run build && node dist/start.js"
  },
  "devDependencies": {
    "@types/escodegen": "^0.0.10",
    "@types/esprima": "^4.0.6",
    "@types/estree": "^1.0.8",
    "@types/node": "^24.10.0",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@playwright/test": "^1.56.1",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "axe-playwright": "^2.2.2",
    "escodegen": "^2.1.0",
    "esprima": "^4.0.1",
    "playwright": "^1.57.0",
    "web-vitals": "^5.1.0",
    "yaml": "^2.8.1"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  }
}